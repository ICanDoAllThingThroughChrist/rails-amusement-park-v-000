<%= form_for @attraction, url: {action: "create"}, html: {class: "nifty_form"} do |f| %>
  Name: <%= f.text_field :name %><br>
  min_height: <%= f.text_field :min_height %><br>
  happiness_rating: <%= f.text_field :happiness_rating %><br>
  nausea_rating: <%= f.text_field  :nausea_rating %><br>
  tickets <%= f.text_field  :tickets %><br>
  <%= f.submit "Create Attraction" %>
<% end %>